# Pre-requisites
mattmc3/zephyr path:plugins/zfunctions

# Zephyr
mattmc3/zephyr path:plugins/terminal
mattmc3/zephyr path:plugins/editor
mattmc3/zephyr path:plugins/history
mattmc3/zephyr path:plugins/directory
mattmc3/zephyr path:plugins/color
mattmc3/zephyr path:plugins/utility
mattmc3/zephyr path:plugins/prompt
mattmc3/zephyr path:plugins/clipboard
mattmc3/zephyr path:plugins/homebrew conditional:is-macos

# Custom plugins
$ZDOTDIR/plugins/nvm kind:defer
$ZDOTDIR/plugins/fzf kind:defer
$ZDOTDIR/plugins/cargo

# Completions
zsh-users/zsh-completions kind:fpath path:src
docker/cli kind:fpath path:contrib/completion/zsh
mattmc3/zephyr path:plugins/completion

# Overrides
mattmc3/zephyr path:plugins/confd

# Deferred plugins
zdharma-continuum/fast-syntax-highlighting kind:defer
zsh-users/zsh-autosuggestions kind:defer
hlissner/zsh-autopair kind:defer

# vim: ft=zsh sw=2 ts=2 et
